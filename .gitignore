# Ignore Python virtual environment
.venv-gpu/

# Ignore pycache files and compiled Python files
__pycache__/
*.pyc

# Ignore training run artifacts, logs, and datasets that are too large
runs/detect/
runs/train/args.yaml
runs/train/results.csv
